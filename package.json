{
  "name": "myapp",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node bin/www",
    "dev": "nodemon -e js, pug bin/www",
    "serverless": "serverless offline --noPrependStageInUrl",
    "db:migrate": "npx prisma migrate dev",
    "db:seed": "node models/seed.js"
  },
  "dependencies": {
    "@prisma/client": "^4.6.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^16.0.3",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "prisma": "^4.6.0",
    "pug": "2.0.0-beta11",
    "serverless-http": "^3.1.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.20",
    "serverless-offline": "^11.3.0"
  },
  "prisma": {
    "schema": "app/models/schema.prisma",
    "seed": "node app/models/seed.js"
  }
}
